{
    "name": "limingxinleo/swoft-aop-cacheable",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "php",
        "swoole",
        "swoft",
        "db",
        "model"
    ],
    "description": "Swoft 基于Aop缓存扩展",
    "authors": [
        {
            "name": "李铭昕",
            "email": "715557344@qq.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Swoftx\\Aop\\Cacheable\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SwoftTest\\": "tests"
        }
    },
    "require": {
        "php": ">=7.0",
        "swoft/framework": "^1.0.24",
        "swoft/redis": "^1.0"
    },
    "require-dev": {
        "swoft/swoole-ide-helper": "dev-master",
        "phpunit/phpunit": "^5.7"
    },
    "extra": {
        "config-plugin": {
            "swoft.components": [
                "config/components.php"
            ]
        }
    }
}
